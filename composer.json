{
    "name": "clvarley/example-php",
    "type": "library",
    "license": "MIT",
    "require": {
        "php": "^8.3"
    },
    "require-dev": {
        "vimeo/psalm": "^6.14",
        "phpunit/phpunit": "^12.5",
        "friendsofphp/php-cs-fixer": "^3.92"
    },
    "autoload": {
        "psr-4": {
            "Clvarley\\Utility\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "tests": "@php vendor/bin/phpunit",
        "style": "@php vendor/bin/php-cs-fixer fix --diff",
        "psalm": "@php vendor/bin/psalm --show-info=true"
    }
}
